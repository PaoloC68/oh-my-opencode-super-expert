  "mcp": {
    "serena": {
      "type": "local",
      "command": [
        "uvx",
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--enable-web-dashboard",
        "false"
      ],
      "enabled": true
    },
    "sequentialthinking": {
      "type": "local",
      "command": [
        "npx",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "enabled": true
    },
    "context7": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@upstash/context7-mcp",
        "--api-key",
        "ctx7sk-"
      ],
      "enabled": true
    }
  }
